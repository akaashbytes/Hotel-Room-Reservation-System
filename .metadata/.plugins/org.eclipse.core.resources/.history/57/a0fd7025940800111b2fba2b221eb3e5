Table creation :

    CREATE TABLE ROOMRES_TB (
    RECORDID VARCHAR2(20) PRIMARY KEY,
    GUESTNAME VARCHAR2(40) NOT NULL,
    ROOMTYPE VARCHAR2(20) NOT NULL,
    CHECKIN_DATE DATE NOT NULL,
    CHECKOUT_DATE DATE NOT NULL,
    ROOMNO VARCHAR2(10),
    REMARKS VARCHAR2(100)
);

Sequence:

CREATE SEQUENCE ROOMRES_SEQ
START WITH 10
INCREMENT BY 1
MAXVALUE 9999
NOCYCLE;
