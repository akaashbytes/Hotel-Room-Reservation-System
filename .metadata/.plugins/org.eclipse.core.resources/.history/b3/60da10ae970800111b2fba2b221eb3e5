Table creation :

CREATE TABLE ROOM_RESERVATION (
    RECORDID NUMBER PRIMARY KEY,
    GUESTNAME VARCHAR2(50),
    ROOMTYPE VARCHAR2(30),
    CHECKIN_DATE DATE,
    CHECKOUT_DATE DATE,
    ROOMNO VARCHAR2(10),
    REMARKS VARCHAR2(200)
);

Sequence:

CREATE SEQUENCE ROOMRES_SEQ
START WITH 10
INCREMENT BY 1
MAXVALUE 9999
NOCYCLE;


CREATE TABLE ROOM_RESERVATION (
    RECORDID        VARCHAR2(20) PRIMARY KEY,
    GUESTNAME       VARCHAR2(50) NOT NULL,
    ROOMTYPE        VARCHAR2(30) NOT NULL,
    CHECKIN_DATE    DATE NOT NULL,
    CHECKOUT_DATE   DATE NOT NULL,
    ROOMNO          VARCHAR2(10) NOT NULL,
    REMARKS         VARCHAR2(200)
);
