CREATE TABLE ROOMRES_TB (
    RECORDID        VARCHAR2(12 BYTE) PRIMARY KEY,
    GUESTNAME       VARCHAR2(40 BYTE) NOT NULL,
    ROOMTYPE        VARCHAR2(20 BYTE) NOT NULL,
    CHECKIN_DATE    DATE NOT NULL,
    CHECKOUT_DATE   DATE NOT NULL,
    ROOMNO          VARCHAR2(10 BYTE),
    REMARKS         VARCHAR2(100 BYTE)
);

CREATE SEQUENCE ROOMRES_SEQ
START WITH 10
INCREMENT BY 1
MAXVALUE 99
NOCYCLE;
